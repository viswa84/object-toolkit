{
  "name": "object-toolkit",
  "version": "1.0.0",
  "main": "dist/cjs",
  "module": "dist/esm",
  "types": "dist/types",
  "author": "Segun Adebayo",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "yarn build",
    "prebuild": "rm -rf dist",
    "build": "concurrently yarn:build:*",
    "test": "jest",
    "test:cov": "jest --coverage",
    "version": "yarn build",
    "build:esm": "tsc --module esnext --outDir dist/esm --declaration false",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs --declaration false",
    "build:types": "tsc --emitDeclarationOnly --declaration true --declarationDir dist/types",
    "lint": "tsc --noEmit"
  },
  "dependencies": {
    "@types/get-value": "3.0.0",
    "@types/set-value": "2.0.0",
    "deepmerge": "4.2.2",
    "get-value": "3.0.1",
    "has-own-deep": "^1.1.0",
    "deep-rename-keys": "0.2.1",
    "set-value": "4.0.1",
    "unset-value": "1.0.0",
    "deep-copy": "1.4.2",
    "dequal": "1.0.0"
  },
  "devDependencies": {
    "@types/jest": "22.2.3",
    "concurrently": "5.1.0",
    "eslint": "6.8.0",
    "jest": "25.2.7",
    "semantic-release": "^17.0.6",
    "ts-jest": "25.3.1",
    "typescript": "^3.8.3"
  }
}
